{
  "tiers": {
    "light": {
      "agents": ["coordinator"],
      "description": "Single-step ops: counts, status checks, fetches. Handle directly or use Gemini CLI."
    },
    "medium": {
      "agents": ["researcher", "sysadmin"],
      "description": "Multi-step research, summarization, documentation, file management."
    },
    "heavy": {
      "agents": ["coder", "sysadmin"],
      "description": "Architecture, complex debugging, multi-file refactors."
    },
    "heavy-code": {
      "agents": ["coder"],
      "description": "Code reviews, test generation, security audits."
    },
    "critical": {
      "agents": ["coder"],
      "description": "Novel problems, production crises. Override model to critical-tier if needed.",
      "modelOverride": "google-antigravity/claude-opus-4-6-thinking"
    }
  },
  "keywords": {
    "light": ["count", "list", "status", "fetch", "check", "read"],
    "medium": ["research", "summarize", "explain", "draft", "organize", "clean"],
    "heavy": ["architecture", "debug", "refactor", "optimize", "migrate"],
    "heavy-code": ["review", "test", "audit", "generate code"],
    "critical": ["crisis", "novel", "security", "production", "high-stakes"]
  },
  "logic": {
    "preferFree": true,
    "maxParallel": 8,
    "batchThreshold": 5
  }
}
